@using FinalProject.ViewModels.Search;

@{
    ViewData["Title"] = "Index";
}

@model SearchViewModel

<div class="content">
    <div class="top-side">
        <div class="search-side">
            <div class="custom-container">
                <form method="post" class="searchbar" asp-controller="Search" asp-action="Index">
                    <div class="searchbar">
                        <div class="search-icon">
                            <i class="fa-solid fa-magnifying-glass"></i>
                        </div>
                        <div class="search-input">
                            <input asp-for="Search" type="text" placeholder="Search">

                        </div>
                    </div>
                </form>
                
            </div>
        </div>
        <div class="for-side">
            <div class="for-you">
                <div class="elements list-group">
                    <a class="element active" asp-action="Filter" asp-route-TabId="Top">
                        <div class="element-inside">
                            <div class="text @(ViewBag.TabId == "Top" ? "active" : "")">
                                <span>Top</span>
                                <div class="bottom-line"></div>
                            </div>
                        </div>
                    </a>
                    <a class="element" asp-action="Filter" asp-route-TabId="Latest">
                        <div class="element-inside">
                            <div class="text @(ViewBag.TabId == "Latest" ? "active" : "")">
                                <span>Latest</span>
                                <div class="bottom-line"></div>
                            </div>
                        </div>
                    </a>
                    <a class="element" asp-action="Filter" asp-route-TabId="People">
                        <div class="element-inside">
                            <div class="text @(ViewBag.TabId == "People" ? "active" : "")">
                                <span> People</span>
                                <div class="bottom-line"></div>
                            </div>
                        </div>
                    </a>
                    <a class="element" asp-action="Filter" asp-route-TabId="Lists">
