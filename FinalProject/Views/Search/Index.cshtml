                            <div class="text @(ViewBag.TabId == "Lists" ? "active" : "")">
                                <span> Lists</span>
                                <div class="bottom-line"></div>
                            </div>
                        </div>
                    </a>
                </div>
            </div>
        </div>
    </div>

